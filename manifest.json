{
	"manifest_version": 2,

  "name": "extension",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "background" : {
    "scripts" : ["background.js"],  //, "eventPage.js"
    "persistent": false
  },
   "browser_action": {
      "default_icon": {
      	"19":"adAlt.png",
      	"38":"adAlt38.png"
      },
      "default_title": "Extension"
    },

    "permissions": [ "webNavigation", "tabs"]
}
